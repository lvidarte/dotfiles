# ============================================================================
# BLOCK
# ============================================================================
snippet b
	<${1:div}${2}>${3}</$1>
snippet B
	<${1:div}${2}>
	</$1>
snippet b_
	<${1:div} class="${3}">${4}</$1>
snippet bb_
	<${1:div} class="${3}">
	</$1>
snippet h
	<h${1:1}${2}>${3}</h$1>
snippet p
	<p>${1}</p>
snippet div
	<div class="${1}">${2}</div>
snippet div_
	<div class="${1}">
		${2}
	</div>
# ============================================================================
# INLINE
# ============================================================================
snippet i
	<${1:span}>${2}</$1>${3}
snippet i_
	<${1:span} id="${2}" class="${3}">${4}</$1>${5}
snippet em
	<em>${1}</em>
snippet strong
	<strong>${1}</strong>
# ============================================================================
# LINKS
# ============================================================================
snippet a
	<a href="${1}"${2}>${3}</a>
snippet mailto
	<a href="mailto:${1:email?subject=feedback}" title="${2}">${3:$1}</a>
snippet mailto_
	<a href="mailto:${1:email?subject=feedback}" title="${2}" id="${3}" class="${4}">${5:$1}</a>
# ============================================================================
# LISTS
# ============================================================================
snippet li
	<li>${1}</li>
snippet li_
	<li>
		${1}
	</li>
snippet ol
	<ol>
		${1}
	</ol>
snippet ul
	<ul>
		${1}
	</ul>
# ============================================================================
# IMAGE
# ============================================================================
snippet img
	<img src="${1}"${2}>${3}
# ============================================================================
# BR
# ============================================================================
snippet br
	<br />
# ============================================================================
# FORM
# ============================================================================
snippet form
	<form action="${1}" method="${2:post}" name="${3:}" id="${4:$3}">${5}
	</form>
snippet form_
	<form action="${1}" method="${2:post}" name="${3:}" id="${4:$3}" accept-charset="utf-8">
	  <p><input type="submit" value="Submit" name="submit" /></p>
	</form>
snippet label
	<label for="${1:id}">${2:label}</label>
snippet in
	<input type="${1:text}" name="${2}"${3}>
snippet input
	<input type="${1:text}" name="${2:}" id="${3:$2}" value="${4}" />${5}
snippet textarea
	<textarea name="${1:}" id="${2:$1}" rows="${3:8}" cols="${4:40}">${5}</textarea>${6}
snippet select
	<select name="${1:}" id="${2:$1}">${3}
	</select>
snippet option
	<option value="${1:}">${2:$1}</option>${3}
# ============================================================================
# HTML
# ============================================================================
snippet html
	<!doctype html>
	<html>
		<head>
			<meta charset="utf-8">
			<title>${1:`substitute(Filename('', 'Page Title'), '^.', '\u&', '')`}</title>
		</head>
		<body>
		</body>
	</html>
snippet htmla
	<!doctype html>
	<html>
		<head>
			<title>${1}</title>
			<script src="https://ajax.googleapis.com/ajax/libs/angularjs/1.3.15/angular.min.js"></script>
			<script src="${2}"></script>
			<link rel="stylesheet" href="${3}">
		</head>
		<body>
		</body>
	</html>
snippet htmljq 
	<!doctype html>
	<html>
		<head>
			<meta charset="utf-8">
			<title>${1:`substitute(Filename('', 'Page Title'), '^.', '\u&', '')`}</title>
			<script src="https://ajax.googleapis.com/ajax/libs/jquery/1.7.1/jquery.min.js"></script>
			<script>
			$(function() {
				${2}
			})
			</script>
		</head>
		<body>
		</body>
	</html>
snippet htmlcs
	<!doctype html>
	<html>
		<head>
			<meta charset="utf-8">
			<title>${1:`substitute(Filename('', 'Page Title'), '^.', '\u&', '')`}</title>
			<script src="https://ajax.googleapis.com/ajax/libs/jquery/1.7.1/jquery.min.js"></script>
			<script src="http://jashkenas.github.com/coffee-script/extras/coffee-script.js"></script>
			<script type="text/coffeescript">
			$->
				${2}
			</script>
		</head>
		<body>
		</body>
	</html>
snippet xhtml
	<?xml version="1.0" encoding="UTF-8"?>
	<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Strict//EN
		"http://www.w3.org/TR/xhtml1/DTD/xhtml1-strict.dtd">
	<html xmlns="http://www.w3.org/1999/xhtml">

		<head>
			<title>${1:`substitute(Filename('', 'Page Title'), '^.', '\u&', '')`}</title>
			<meta http-equiv="Content-Type" content="text/html;charset=utf-8" />
		</head>

		<body>

			${2}

		</body>
	</html>
snippet xhtml_
	<?xml version="1.0" encoding="UTF-8"?>
	<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Strict//EN
		"http://www.w3.org/TR/xhtml1/DTD/xhtml1-strict.dtd">
	<html xmlns="http://www.w3.org/1999/xhtml">

		<head>
			<title>${1:`substitute(Filename('', 'Page Title'), '^.', '\u&', '')`}</title>

			<meta http-equiv="Content-Type" content="text/html;charset=utf-8" />
			<meta name="keywords" content="" />
			<meta name="description" content="" />
			<meta name="author" content="`g:snips_author`" />

			<script type="text/javascript" src="${2}"></script>
			<link rel="stylesheet" href="${3}" type="text/css" media="screen" />
		</head>

		<body>

			${4}

		</body>
	</html>
snippet head
	<head>
		<meta http-equiv="Content-type" content="text/html; charset=utf-8"/>
		<title>${1:Page Title}</title>
		${2}
	</head>
snippet meta
	<meta name="${1}" content="${2}" />
snippet body
	<body id="${1}">
		${2}
	</body>
