# ============================================================================
# TORNADO
# ============================================================================
snippet _
	{{ ${1} }}${2}
snippet __
	{% ${1} %}${2}
snippet ___
	{% ${1:tag_name} %}
	{% end %}
snippet apply
	{% apply ${1:function} %}${2}{% end %}
snippet autoescape
	{% autoescape ${1:function} %}
snippet bk
	{% block ${1:name} %}
	{% end %}
snippet block
	{% block ${1:name} %}
	{% end %}
snippet bk_
	{% block ${1:name} %}${2}{% end %}
snippet block_
	{% block ${1:name} %}${2}{% end %}
snippet #
	{% comment ${1} %}
snippet #_
	{% comment 
		${1}
	%}
snippet elif
	{% elif ${1:condition} %}
snippet else
	{% else %}
snippet end
	{% end %}
snippet ext
	{% extends "${1:filename}" %}
snippet extends
	{% extends "${1:filename}" %}
snippet for
	{% for ${1:var} in ${2:exp} %}
	{% end %}
snippet for_
	{% for ${1:var} in ${2:exp} %}${3}{% end %}
snippet from
	{% from ${1:module} import ${2:name} %}
snippet if
	{% if ${1:condition} %}
	{% end %}
snippet if_
	{% if ${1:condition} %}${2}{% end %}
snippet import
	{% import ${1:module} %}
snippet inc
	{% include "${1:filename}" %}
snippet include
	{% include "${1:filename}" %}
snippet module
	{% module ${1:expr} %}
snippet raw
	{% raw ${1:expr} %}
snippet set
	{% set ${1:x}=${2:y} %}
snippet try
	{% try %}
		${1:expr}
	{% except ${2} %}
		${3:expr}
	{% end %}
snippet try_
	{% try %}
		${1:expr}
	{% except ${2} %}
		${3:expr}
	{% finally %}
		${4:expr}
	{% end %}
snippet while
	{% while ${1:condition} %}
	{% end %}
snippet while_
	{% while ${1:condition} %}${2}{% end %}
